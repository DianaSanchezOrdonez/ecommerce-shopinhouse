oryID }) => {\r\n  const [arrayProducts, setArrayProducts] = useState([0]);\r\n  const [isLoading, setIsLoading] = useState(false);\r\n  const { products } = useContext(CartContext);\r\n\r\n  useEffect(() => {\r\n    setIsLoading(true);\r\n\r\n    if (!categoryID) {\r\n      setArrayProducts(products);\r\n      setIsLoading(false);\r\n    } else  {\r\n      let productByCategory = products.filter(\r\n        (product) => product.categoryId === categoryID\r\n      );\r\n      setArrayProducts(productByCategory);\r\n      setIsLoading(false);\r\n    }\r\n\r\n    return () => {};\r\n  }, [categoryID]);\r\n\r\n  if (isLoading) {\r\n    return <Loader />;\r\n  }\r\n\r\n  return(\r\n    arrayProducts.map((product) => { return <ItemList product={product} key={product.id} /> })\r\n  )\r\n};\r\n\r\nexport default ItemListContainer;\r\n","C:\\coderhouse\\ecommerce-shopinhouse\\src\\components\\itemcount\\ItemCount.jsx",[],["160","161"],"C:\\coderhouse\\ecommerce-shopinhouse\\src\\pages\\notfound\\NotFound.jsx",[],"C:\\coderhouse\\ecommerce-shopinhouse\\src\\containers\\itemdetailcontainer\\ItemDetailContainer.jsx",[],"C:\\coderhouse\\ecommerce-shopinhouse\\src\\components\\itemlist\\ItemList.jsx",[],"C:\\coderhouse\\ecommerce-shopinhouse\\src\\components\\itemdetail\\ItemDetail.jsx",["162"],"import React, { useState, useContext } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { Alert } from \"react-bootstrap\";\r\n\r\nimport \"./itemdetail.css\";\r\nimport ItemCount from \"../itemcount/ItemCount\";\r\nimport Opinions from \"../opinions/Opinions\";\r\n\r\n/**Contexto */\r\nimport { CartContext } from \"../../context/CartContext\";\r\nimport {FavoriteContext} from \"../../context/FavoriteContext\";\r\n\r\nconst ItemDetail = ({ item }) => {\r\n  const [count, setCount] = useState(1);\r\n  const [statusButton, setStatusButton] = useState(false);\r\n  const [show, setShow] = useState(false);\r\n  const [showFavorite, setShowFavorite] = useState(false);\r\n\r\n  const history = useHistory();\r\n\r\n  const { methods } = useContext(CartContext);\r\n  const { methodsFavorite } = useContext(FavoriteContext);\r\n\r\n  const handleInput = (name, value) => {\r\n    if (value <= item.stock) {\r\n      setCount(value);\r\n    } else {\r\n      setShow(true);\r\n    }\r\n  };\r\n\r\n  const onAdd = (count) => {\r\n    setStatusButton(true);\r\n    methods.addItem({ item: item, quantity: count }, count);\r\n  };\r\n\r\n  const onAddFavorite = () => {\r\n    methodsFavorite.addItemFavorite({ item: item });\r\n    setShowFavorite(true)\r\n  };\r\n\r\n  return item.categoryId ? (\r\n    <>\r\n    <div className=\"d-flex-row\">\r\n      {\r\n        show ? <Alert variant=\"danger\" onClose={() => setShow(false)} dismissible>\r\n                <Alert.Heading>Take care!</Alert.Heading>\r\n                <p>\r\n                  The quantity of items cannot exceed the stock. <br/> \r\n                  Change this and that and try again.\r\n                </p>\r\n              </Alert>\r\n              : \"\"\r\n      }\r\n      {\r\n        showFavorite ? <Alert onClose={() => setShowFavorite(false)} variant=\"success\" dismissible>\r\n                Added to{' '}\r\n                <Alert.Link href=\"/favorite\">Favorites</Alert.Link>. Give it a click if you\r\n                like.\r\n               </Alert>\r\n              : \"\"\r\n      }\r\n      <main className=\"main-class\">\r\n        <div className=\"slider-img\">\r\n          <img src={item.image} alt={item.description} />\r\n        </div>\r\n      </main>\r\n      <aside className=\"aside-class\">\r\n        <div className=\"description\">\r\n          <h2>{item.title}</h2>\r\n          <p>Stock {item.stock}</p>\r\n          <hr />\r\n          <p>{item.description}</p>\r\n          <div className=\"count-price\">\r\n            <label>Price: </label>\r\n            <span>$ {item.price * count}</span>\r\n          </div>\r\n          {statusButton ? (\r\n    